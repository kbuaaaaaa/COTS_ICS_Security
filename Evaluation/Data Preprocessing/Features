Date (get from netml, use as keys)
Time (get from netml, use as keys)
Eth_dst : .eth.dst
Eth_src : .eth.src
Eth_type : .eth.type
IP_src : .ip.src
IP_dst : .ip.dst
IP_ver : .ip.version
IP_hl : .ip.hdr_len
IP_dsf : .ip.dsfield
IP_len : .ip.len
IP_id : .ip.id
IP_flags : .ip.flags
IP_frag : .ip.frag_offset
IP_ttl : .ip.tll
IP_proto (get from wireshark csv)
TCP_sport : .tcp.srcport
TCP_dport : .tcp.dstport
TCP_seq : .tcp.seq
TCP_ack : .tcp.ack
TCP_hl : .tcp.hdr_len
TCP_flags : .tcp.flags
TCP_windows : .tcp.wondow_size
TCP_timedelta :
TCP_timerelative : 
RAW : 
Attack : bool

filter list : https://medium.com/hacker-toolbelt/wireshark-filters-list-983c49468a45

tshark -r normal_injection.pcap -E header=y -E separator=, -T fields -e eth.src -e eth.dst -e eth.type -e ip.src -e ip.dst -e ip.version -e ip.hdr_len -e ip.dsfield -e ip.len -e ip.id -e ip.flags -e ip.frag_offset -e ip.ttl -e ip.proto -e tcp.srcport -e tcp.dstport -e tcp.seq -e tcp.ack -e tcp.hdr_len -e tcp.flags -e tcp.window_size -e tcp.time_delta -e tcp.time_relative > normal_injection.csv

tshark -r recon_attack.pcap -E header=y -E separator=, -T fields -e frame.time -e tcp.dstport > recon_attack.csv